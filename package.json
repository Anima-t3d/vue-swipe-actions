{
  "name": "vue-swipe-actions",
  "description": "iOS style swipe actions",
  "version": "2.0.0-beta.5",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build",
    "build-bundle": "vue-cli-service --mode production build --target lib --name vue-swipe-actions ./src/components/index.js",
    "lint": "vue-cli-service lint",
    "deploy": "gh-pages-deploy"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ]
  },
  "author": {
    "name": "eCollect AG"
  },
  "keywords": [
    "vuejs",
    "vue-swipe-actions",
    "vue",
    "swipe",
    "list"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eCollect/vue-swipe-actions"
  },
  "main": "./dist/vue-swipe-actions.common.js",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "eslint": "^5.15.1",
    "eslint-config-ecollect-vue": "^0.1.0-beta.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "gh-pages-deploy": "^0.5.1",
    "vue-template-compiler": "^2.6.x"
  },
  "peerDependencies": {
    "vue": "^2.6.x"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "license": "MIT",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "files": [
    "dist/vue-swipe-actions.common.js",
    "dist/vue-swipe-actions.css"
  ],
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "vue": "^2.6.8"
  }
}
