{
  "name": "vue-swipe-actions",
  "description": "iOS style swipe actions",
  "version": "2.0.0-beta.9",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ./demo/src/main.js",
    "build-demo": "NODE_ENV=production vue-cli-service build ./demo/src/main.js --dest ./demo/dist",
    "build-bundle": "node build/build.js",
    "lint": "vue-cli-service lint",
		"deploy": "gh-pages-deploy"
  },
  "gh-pages-deploy": {
    "staticpath": "demo/dist",
    "prep": [
      "build-demo"
    ]
  },
  "author": {
    "name": "eCollect AG"
  },
  "keywords": [
    "vuejs",
    "vue-swipe-actions",
    "vue",
    "swipe",
    "list"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eCollect/vue-swipe-actions"
  },
  "main": "./dist/vue-swipe-actions.common.js",
  "module": "./src/index.esm.js",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.15.1",
    "eslint-config-ecollect-vue": "^0.1.0-beta.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "gh-pages-deploy": "^0.5.1",
    "postcss": "^7.0.16",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "uglify-es": "^3.3.9",
    "vue-template-compiler": "^2.6.x"
  },
  "peerDependencies": {
    "vue": "^2.6.x"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "files": [
    "dist",
    "src"
